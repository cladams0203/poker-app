mponents/PlayerPosition.tsx",[],"/Users/chris/Desktop/pokerApp/poker-app/src/utils/helpers/computerPlayers.ts",[],"/Users/chris/Desktop/pokerApp/poker-app/src/utils/helpers/index.ts",[],"/Users/chris/Desktop/pokerApp/poker-app/src/components/User.tsx",["99"],"import { AppState, Card, PlayerPositionProps } from \"../types\";\nimport \"../styles/User.scss\";\nimport { useState } from \"react\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { BET } from \"../state/actions/gameActions\";\n\nexport const User: React.FC = () => {\n  const state = useSelector((state: AppState) => state.game);\n  const user = state.players[0];\n  const dispatch = useDispatch();\n  const [raise, setRaise] = useState({ allow: false, amount: 0 });\n  const call = () => {\n    dispatch({\n      type: BET,\n      payload: { id: user.playerId, amt: state.currentBet },\n    });\n  };\n\n  const enableRaise = () => {\n    setRaise({ ...raise, allow: true, amount: state.currentBet });\n  };\n\n  const submitRaise = (e: React.FormEvent<HTMLFormElement>) => {\n    e.preventDefault();\n    dispatch({\n      type: BET,\n      payload: { id: user.playerId, amt: raise.amount },\n    });\n    setRaise({ allow: false, amount: 0 });\n  };\n\n  return (\n    <>\n      {user && (\n        <div className=\"active-user\">\n          {user.isButton && <div className=\"user-button\">D</div>}\n          <div className=\"user-cards\">\n            {user.currentHand.map((item: Card) => (\n              <img src={item.image} alt={item.displayValue} />\n            ))}\n          </div>\n          <p className=\"user-chips\">Chips: {user.chips}</p>\n          {!raise.allow && (\n            <div>\n              <button>Fold</button>\n              <button onClick={() => call()}>Call</button>\n              <button onClick={() => enableRaise()}>Raise</button>\n            </div>\n          )}\n          {raise.allow && (\n            <form onSubmit={submitRaise}>\n              <label>\n                Raise Ammount:\n                <input\n                  type=\"number\"\n                  value={raise.amount && raise.amount}\n                  onChange={(e) =>\n                    setRaise({ ...raise, amount: Number(e.target.value) })\n                  }\n                />\n              </label>\n              <button type=\"submit\">Raise</button>\n              <button onClick={() => setRaise({ ...raise, allow: false })}>\n                Cancel\n              </button>\n            </form>\n          )}\n        </div>\n      )}\n    </>\n  );\n};\n","/Users/chris/Desktop/pokerApp/poker-app/src/components/Dealer.tsx",[],"/Users/chris/Desktop/pokerApp/poker-app/src/components/PlayerLayout.tsx",["100"],"import { PlayerPosition } from \"./PlayerPosition\";\nimport { useSelector } from \"react-redux\";\nimport { AppState, Player } from \"../types\";\nimport \"../styles/PlayerLayout.scss\";\nimport { User } from \"./User\";\nexport const PlayerLayout: React.FC = () => {\n  const state = useSelector((state: AppState) => state.game);\n  return (\n    <div className=\"player-layout\">\n      <User />\n      {state.players.map((item: Player, idx) => {\n        if (item.activeUser === false) {\n          return <PlayerPosition idx={idx} key={item.playerId} player={item} />;\n        }\n      })}\n    </div>\n  );\n};\n","/Users/chris/Desktop/pokerApp/poker-app/src/components/Register.tsx",["101"],"/Users/chris/Desktop/pokerApp/poker-app/src/utils/auth/api.ts",[],"/Users/chris/Desktop/pokerApp/poker-app/src/components/Login.tsx",["102","103"],"/Users/chris/Desktop/pokerApp/poker-app/src/utils/auth/ProtectedRoute.tsx",[],"/Users/chris/Desktop/pokerApp/poker-app/src/state/actions/userActions.ts",[],"/Users/chris/Desktop/pokerApp/poker-app/src/state/reducers/userReducer.ts",[],"/Users/chris/Desktop/pokerApp/poker-app/src/components/Dashboard.tsx",["104","105"],{"ruleId":"106","replacedBy":"107"},{"ruleId":"108","replacedBy":"109"},{"ruleId":"110","severity":1,"me